# Controller-driven landing worker (infra env only; 策略由 controller 下发)
ENV=staging
ROLE=landing
INSTANCE_ID=landing-dev-1
APP_NAME=alist-landing-worker
APP_VERSION=dev

# Controller access
CONTROLLER_URL=https://controller.example.com
CONTROLLER_API_PREFIX=/api/v0
CONTROLLER_API_TOKEN=replace-with-controller-token

# Cache/DO wiring (绑定名需与 wrangler 对应)
BOOTSTRAP_CACHE_MODE=d1
METRICS_MODE=do
# D1 cache（可选；INIT_TABLES=true 仅在开发/新环境自动建表）
CACHE_D1=CACHE_D1
INIT_TABLES=false
BOOTSTRAP_DO=BootstrapDO
METRICS_DO=MetricsDO
BOOTSTRAP_KV=BOOTSTRAP_KV

# Internal control API
INTERNAL_API_TOKEN=replace-with-internal-token

# Optional: Cloudflare native rate limiter
ENABLE_CF_RATELIMITER=false
CF_RATELIMITER_BINDING=CF_RATE_LIMITER

# 验证链/加密/限流/路径策略均由 controller.common/landing 下发，勿在 env 配置策略字段。
